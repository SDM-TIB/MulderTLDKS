PREFIX bsbm-inst: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/>
PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX rev: <http://purl.org/stuff/rev#>
SELECT DISTINCT ?product ?revText ?rating3 ?plabel
WHERE {
?product rdfs:label ?plabel .
?product bsbm:producer <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer5246/Producer5246> .
?product a ?productType .
<http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer5246/Producer5246> a bsbm:Producer .
<http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer5246/Producer5246> rdfs:label ?prlabel .
<http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/dataFromProducer5246/Producer5246> foaf:homepage ?homepage .
?review bsbm:reviewFor  ?product .
?review bsbm:rating1 ?rating1 .
?review bsbm:rating2 ?rating2 .
?review bsbm:rating3 ?rating3 .
?review rev:text ?revText .
}
